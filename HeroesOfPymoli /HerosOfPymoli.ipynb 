    "male_count_df = df.loc[df['Gender'] == 'Male' ]\n",
    "male_count1 = len(male_count_df)\n",
    "\n",
    "female_count_df = df.loc[df['Gender'] == 'Female']\n",
    "female_count1 = len(female_count_df)\n",
    "\n",
    "other_count_df = df.loc[df['Gender'] == 'Other / Non-Disclosed']\n",
    "other_count1 = len(other_count_df)\n",
    "\n",
    "gender_grp = df.groupby(['Gender'])\n",
    "gender_grp_avg = gender_grp.mean()\n",
    "gender_grp_tot = gender_grp.sum()\n",
    "avg_price_male = gender_grp_avg.iloc[0, 3]\n",
    "avg_price_female = gender_grp_avg.iloc[1, 3]\n",
    "avg_price_other = gender_grp_avg.iloc[2, 3]\n",
    "total_price_male = gender_grp_tot.iloc[0, 3]\n",
    "total_price_female = gender_grp_tot.iloc[1, 3]\n",
    "total_price_other = gender_grp_tot.iloc[2, 3]\n",
    "\n",
    "\n"
   ]
