    "age_df[\"Age_Group\"] = pd.cut(age_df[\"Age\"], bins, bin_labels)\n",
    "age_group = age_df.groupby(\"Age_Group\")\n",
    "age_group_df = age_group.count()\n",
    "age_group_df = age_group_df.rename(columns={\"Age\" : \"Total Count\"})\n",
    "#this next line didnt work?\n",
    "age_group_df = age_group_df.rename(index={0: \"<10\", 1: \"10-14\", 2: \"14-19\", 3: \"20-24\", 4: \"25-29\", 5: \"30-34\", 6: \"35-39\", 7: \"40+\"})\n",
    "\n",
    "age_group_df[\"Percentage of Players\"] = (100 * age_group_df[\"Total Count\"] / age_group_df[\"Total Count\"].sum()).round(2)\n",
